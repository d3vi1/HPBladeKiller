<RIBCL VERSION="2.0">
   <LOGIN USER_LOGIN="Administrator" PASSWORD="Not Required Because The OA Uses SSO">
      <RIB_INFO MODE="write">
         <LICENSE>
            <ACTIVATE KEY="ABCDE-ABCDE-ABCDE-ABCDE-ABCDE"/>
         </LICENSE>
      </RIB_INFO>
      <RIB_INFO MODE="write">
         <INSERT_VIRTUAL_MEDIA DEVICE="CDROM" IMAGE_URL="https://example.com/hperaser/image.iso"/>
         <SET_VM_STATUS DEVICE="CDROM">
            <VM_BOOT_OPTION VALUE="BOOT_ONCE"/>
            <VM_WRITE_PROTECT VALUE="YES" />
         </SET_VM_STATUS>
      </RIB_INFO>
      <SERVER_INFO MODE="write">
         <SET_HOST_POWER HOST_POWER="Yes"/>
      </SERVER_INFO>
   </LOGIN>
</RIBCL>
